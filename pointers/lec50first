#include<iostream>
using namespace std;

int main()
{
    //array create krna hai jo store krega address
    int n,m;//n=rows  m=col
    cin>>n;
    cin>>m;
    int **ptr=new int *[n];

    //created 2D array
    for(int i=0;i<n;i++)
    {
        ptr[i]=new int [m];
    }

    //filling values inside 2D array
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            cin>>ptr[i][j];
        }
    }

    //printing the values
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
            {

                cout<<ptr[i][j]<<" ";
              
            }
              cout<<endl;
         
    }

    //release the memory,heap mein
    for(int i=0;i<n;i++)
    {

            delete[] ptr[i];
        
    }
    delete[] ptr;

}