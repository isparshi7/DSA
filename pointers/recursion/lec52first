    //print number from 1 to n (even number) by passing only one argument
    #include<iostream>
    using namespace std;
    void even(int N)
    {
        if(N==2)
        {
            cout<<N<<" ";
            return;
        }
        even(N-2);
        cout<<N<<" ";

    }
    int main()
    {
        int n;
        cin>>n;
        if(n%2==1)
        {
            n=n-1;
        }
        even(n);
        return 0;
    }